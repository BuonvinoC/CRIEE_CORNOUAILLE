use database if exists CRIEE_CORNOUAILLES_V1;
Drop trigger if exits refuser_encherir_inferieur;
create trigger if not exists refuser_encherir_inferieur
before update on lot
for each row
BEGIN
    IF ( :old.prix_actuel < :new.prix_actuel )
    THEN
      prix_actuel  = new.prix_actuel  ;
    END
  END;
